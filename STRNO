# cook your dish her
t=int(input())
while(t>0):
    t-=1
    c=0
    n=[int(n) for n in input().split()]
    #print(n[0],n[1])
    for i in range(2,n[0]+1):
        if(i*i<=n[0]):
            while(n[0]%i==0):
                c+=1
                n[0]=n[0]/i
        else:
            break
    if(n[0]>1):
        c+=1
    if(c>=n[1]):
        print("1")
    else:
        print("0")
